{
  "manifest_version": 3,
  "name": "Benchwarmer",
  "version": "1.0.0",
  "permissions": ["sidePanel", "activeTab", "tabs"],
  "host_permissions": ["<all_urls>"],
  "side_panel": { "default_path": "sidepanel.html" },
  "background": { "service_worker": "background.js" },
  "action": { "default_title": "Open Reporter" }
}